
def copy_content(src_content):
    # Simulate copying text (like copying from a file)
    dst_content = src_content
    return dst_content

# Main program
if __name__ == "__main__":
    # Pretend this is the content of a source file
    source_data = "Hello, this is sample content!"

    # Copy operation (simulated)
    destination_data = copy_content(source_data)

    # Show results
    print("Source content:", source_data)
    print("Copied to destination:", destination_data)
