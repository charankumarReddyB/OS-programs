import os
import multiprocessing

def child_process():
    print("Child Process")
    print("PID:", os.getpid())     # Child PID
    print("PPID:", os.getppid())   # Parent PID

if _name_ == "_main_":
    print("Parent Process")
    print("PID:", os.getpid())     # Parent PID
    print("PPID:", os.getppid())   # Parent's Parent PID (like shell/terminal)

    # Create child process
    p = multiprocessing.Process(target=child_process)
    p.start()
    p.join()
