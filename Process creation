import os
from multiprocessing import Process

def child_process():
    print("Child Process:")
    print("PID:", os.getpid())         
    print("Parent PID:", os.getppid()) 


    print("Parent Process:")
    print("PID:", os.getpid())         
    

    # Create a new process
    p = Process(target=child_process)
    p.start()  
    p.join()
child_process()
